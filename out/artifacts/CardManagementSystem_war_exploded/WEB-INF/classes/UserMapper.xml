<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.digital.dao.UserMapper">

    <insert id="addUser"></insert>
    <update id="updateUser"></update>
    <delete id="deleteUser"></delete>
    <select id="selectUserByUsername"></select>
    <select id="queryAllUsers" resultType="com.digital.entity.User">
        select * from cardinfo.user where username=#{username}
    </select>
</mapper>